

.image{
    display: flex;
    width:100%;
    height: 366rpx;
}

.image-overview{
    width:100%;
}

.row-container{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    height: 80rpx;
    align-items: center;
    background-color: white;
    margin-bottom: 10px;
}

.level-container{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    height: 60rpx;
    align-items: center;
    background-color: white;
}


.level-content{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    height: 80rpx;
    align-items: center;
    background-color: white;
    margin-bottom: 10px;

}

.overview {
    display: flex;
    width: 100%;
    flex-direction: column;
    background-color: white;
    
}

.footer {
    display: flex;
    flex-direction: row; 
	justify-content: space-around;
    background: orangered
}
.bottom{
    position: fixed;
    bottom: 0;
    width: 100%;
    color: white;
    background-color: #02b8a9;
}